<div class="card-container">
  <% listings.forEach(listing => { %>
<div class="card mb-3">
  <img src="<%= listing.image?.url || 'https://via.placeholder.com/300' %>" class="card-img-top" alt="Listing Image">
  <div class="card-body">
    <h5 class="card-title"><%= listing.title || "No Title" %></h5>
    <p class="card-text"><%= listing.location || "No Location" %></p>
    <a href="/listings/<%= listing._id %>" class="btn btn-primary">View Details</a>
  </div>
</div>
  <% }); %>
</div>
